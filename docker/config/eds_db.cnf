[mysqld]

# Size of the buffer pool, the memory area where InnoDB caches table and index data
innodb_buffer_pool_size = 128M

# Maximum number of concurrent connections allowed
max_connections = 100

# IP address to which the server should bind
bind-address = 0.0.0.0

# Default character set for tables, column, index, and database names
character_set_server = utf8mb4

# Default collation for string comparison and sorting
collation_server = utf8mb4_unicode_ci